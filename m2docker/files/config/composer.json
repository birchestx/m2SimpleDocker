{
  "name": "alankent/store",
  "require": {
    "magento/product-community-edition": "2.0.0-rc"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.magento.com/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Magento\\Setup\\": "htdocs/setup/module/Magento/Setup/src/"
     "Magento\\Framework\\": "htdocs/lib/internal/Magento/Framework/",
    }
  },
  "extra": {
    "magento-root-dir": "htdocs",
    "magento-deploystrategy": "copy"
  },
  "minimum-stability": "dev"
}
